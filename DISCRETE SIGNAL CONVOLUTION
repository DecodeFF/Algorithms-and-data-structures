#include <iostream>
#include <vector>

using namespace std;

int main() {
  // Вхідні сигнали
  vector<int> x_k = {1, 2, 3};
  vector<int> y_k = {1, 3, 5};

  // Результат згортки
  vector<int> f = vector<int>(x_k.size() + y_k.size() - 1);

  // Розрахунок згортки
  for (int n = 0; n < f.size(); n++) {
    for (int k = 0; k < x_k.size(); k++) {
      if (n - k >= 0) {
        f[n] += x_k[k] * y_k[n - k];
      }
    }
  }

  // Виведення результату
  for (int i = 0; i < f.size(); i++) {
    cout << f[i] << " ";
  }
  cout << endl;

  return 0;
}
