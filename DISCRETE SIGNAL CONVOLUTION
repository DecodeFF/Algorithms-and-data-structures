#include <iostream>
#include <vector>

using namespace std;

int main() {
  // Вхідні сигнали
  vector<int> x_k = {1, 2, 3};
  vector<int> y_k = {5, 3, 1};

  // Результат згортки
  vector<int> f;

  // Розрахунок згортки
  for (int n = 0; n < x_k.size() + y_k.size() - 1; n++) {
    int sum = 0;
    for (int k = 0; k < x_k.size(); k++) {
      if (n - k >= 0) {
        sum += x_k[k] * y_k[n - k];
      }
    }
    f.push_back(sum);
  }

  // Виведення результату
  for (int i = 0; i < f.size(); i++) {
    cout << f[i] << " ";
  }
  cout << endl;

  return 0;
}
